{
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "v1-5-pruned-emaonly.safetensors"
    },
    "_meta": {
      "title": "Load Checkpoint"
    }
  },
  "2": {
    "class_type": "LoraLoader",
    "inputs": {
      "lora_name": "loras/your_lora.safetensors",
      "strength_model": 0.8,
      "strength_clip": 0.8,
      "model": ["1", 0],
      "clip": ["1", 1]
    },
    "_meta": {
      "title": "Load LoRA"
    }
  },
  "3": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "a photograph of a mountain landscape at sunset, detailed, high quality",
      "clip": ["2", 1]
    },
    "_meta": {
      "title": "CLIP Text Encode (Positive)"
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "blurry, low quality, distorted",
      "clip": ["2", 1]
    },
    "_meta": {
      "title": "CLIP Text Encode (Negative)"
    }
  },
  "5": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 512,
      "height": 512,
      "batch_size": 1
    },
    "_meta": {
      "title": "Empty Latent Image"
    }
  },
  "6": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 42,
      "steps": 20,
      "cfg": 7.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["2", 0],
      "positive": ["3", 0],
      "negative": ["4", 0],
      "latent_image": ["5", 0]
    },
    "_meta": {
      "title": "KSampler"
    }
  },
  "7": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["6", 0],
      "vae": ["1", 2]
    },
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "8": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "SD15_lora_txt2img",
      "images": ["7", 0]
    },
    "_meta": {
      "title": "Save Image"
    }
  }
}
